{
  "name": "@auto-send-message/portal",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "vite --mode mock",
    "start:dev": "vite --mode localdev",
    "dev:staging": "vite --mode staging",
    "dev:prod": "vite --mode prod",
    "dev": "vite --mode dev",
    "build": "tsc && node --max_old_space_size=8192 ../../node_modules/vite/bin/vite.js build --emptyOutDir",
    "build:dev": "tsc && node --max_old_space_size=8192 ../../node_modules/vite/bin/vite.js build --mode dev --emptyOutDir",
    "build:staging": "tsc && node --max_old_space_size=8192 ../../node_modules/vite/bin/vite.js build --mode staging --emptyOutDir",
    "lint": "eslint \"**/*.{ts,tsx}\"",
    "preview": "vite preview"
  },
  "dependencies": {
    "@ant-design/charts": "^2.0.3",
    "@auto-send-message/hooks": "*",
    "@reduxjs/toolkit": "^1.9.3",
    "antd": "5.14.1",
    "dayjs": "^1.11.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^14.1.2",
    "react-icons": "^4.8.0",
    "react-player": "2.13.0",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.8.2",
    "redux": "^4.2.1",
    "socket.io-client": "^4.7.5"
  },
  "devDependencies": {
    "@auto-send-message/tsconfig": "*",
    "@types/i18next": "^13.0.0",
    "@types/mime": "3.0.4",
    "@types/mocha": "^10.0.1",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@types/react-redux": "^7.1.25",
    "@types/react-router-dom": "^5.3.3",
    "@types/styled-components": "^5.1.26",
    "@vitejs/plugin-legacy": "^2.0.0",
    "@vitejs/plugin-react": "^2.1.0",
    "styled-components": "^5.3.5",
    "terser": "^5.14.2",
    "typescript": "^4.6.4",
    "vite": "^3.1.0"
  }
}
